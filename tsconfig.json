{
  "compilerOptions": {
    "target": "ES2017",
    "paths": {
      "@/*": ["./*"]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/component/account/Sidebar.jsx", "src/app/component/account/Profile.jsx", "src/app/component/account/AccountLayout.jsx", ".next/types/app/admin/home/page.jsx", ".next/types/app/admin/layout.js", "i18n.mjs", "src/app/lib/dictionary.js"  ],
  "exclude": [
    "node_modules"
  ]
}
